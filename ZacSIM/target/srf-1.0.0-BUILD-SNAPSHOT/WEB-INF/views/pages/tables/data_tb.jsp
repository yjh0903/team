          <div class="row">
            <div class="col-xs-12">
            
              <div class="box">
                <div class="box-header">
                  <h3 class="box-title">Hover Data Table</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                  <table id="example2" class="table table-bordered table-hover">
                    <thead>
                      <tr>
		            	<th>Rendering engine</th>
		           	    <th>Browser</th>
		                <th>Platform(s)</th>
		                <th>Engine version</th>
		                <th>CSS grade</th>
            		  </tr>
                    </thead>
                    <tbody>
                      <!-- <tr>
                        <td>Other browsers</td>
                        <td>All others</td>
